#include <iostream>
#include <string>
using namespace std;

int main() {
    string str, result = " ";
    cout << "Enter the string: ";
    cin>>str;
    for (int i = 0; i < str.size(); i++) {
        if (str[i] != 'a' && str[i] != 'e' && str[i] != 'i' && str[i] != 'o' && str[i] != 'u' &&
            str[i] != 'A' && str[i] != 'E' && str[i] != 'I' && str[i] != 'O' && str[i] != 'U') {
            result += str[i];
        }
    }

    cout << "After removal: " << result << endl;
    return 0;
}
